{
    "name": "@mkrause/lifecycle-loader",
    "version": "0.0.0",
    "description": "Utilities for managing the state lifecycle of async/lazy data",
    "author": "mkrause",
    "license": "MIT",
    "repository": "https://github.com/mkrause/lifecycle",
    "es2015": "src/lifecycle.js",
    "main": "lib-cjs/lifecycle.js",
    "module": "lib-esm/lifecycle.js",
    "browser": "lib-esm/lifecycle.js",
    "files": [
        "src/",
        "lib-cjs/",
        "lib-esm/"
    ],
    "scripts": {
        "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel src --out-dir lib-cjs --source-maps inline",
        "build:esm": "NODE_ENV=production BABEL_ENV=esm babel src --out-dir lib-esm --source-maps inline",
        "test": "NODE_ENV=development BABEL_ENV=cjs mocha --require=@babel/register --recursive tests",
        "prepublishOnly": "npm run build:esm && npm run build:cjs && flow-copy-source src lib-cjs"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.46",
        "@babel/cli": "^7.0.0-beta.46",
        "@babel/register": "^7.0.0-beta.46",
        "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
        "babel-plugin-transform-builtin-extend": "^1.1.2",
        "@babel/preset-env": "^7.0.0-beta.46",
        "@babel/preset-flow": "^7.0.0-beta.46",
        "flow-bin": "^0.71.0",
        "flow-copy-source": "^1.3.0",
        "mocha": "^5.1.1",
        "chai": "^4.1.2"
    },
    "dependencies": {
        "lodash": "^4.17.10",
        "immutable": "^4.0.0-rc.9",
        "message-tag": "^0.2.2"
    }
}
